    <title>fixed and sticky</title>
    <style>
      * {
        padding: 0;
        margin: 0;
        box-sizing: border-box;
      }
      body {
        display: grid;
        grid-template-rows: 80px 1fr 40px;
        /* height: 100vh; */
      }
      header {
        background-color: coral;
        color: white;
        position: sticky;
        top: 0px;
        box-shadow: 0px 1px 1px 1px white;
      }
      main {
        background-color: grey;
        color: white;
        display: grid;
        grid-template-columns: 1fr 20%;
        padding: 10px;
        gap: 10px;
      }
      footer {
        background-color: black;
        color: white;
      }
      .leftSideBlock {
        text-align: justify;
      }
      .advt {
        position: fixed;
      }
      footer {
        text-align: center;
        font-style: italic;
      }
      header {
        display: flex;
        align-items: center;
        justify-content: space-between;
        padding: 10px;
      }
      .logo {
        height: 80px;
        width: 80px;
        display: flex;
        align-items: center;
      }
      /* cards*/
      .cardDepth {
        perspective: 600px;
        height: 300px;
        width: 250px;
        background-color: gray;
      }
      .card {
        height: 100%;
        width: 100%;
        background-color: yellow;
        transform-style: preserve-3d;
        transition: transform 2s;
      }
      .cardDepth:hover .card {
        transform: rotateY(180deg);
      }
      .cardFace {
        height: 100%;
        width: 100%;
        background-color: red;
        position: absolute;
        backface-visibility: hidden;
      }
      video {
        object-fit: cover;
      }
      .front {
        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
        gap: 30px;
        padding: 10px;
        background: coral;
      }
      .front h2 {
        font-size: 15px;
        white-space: nowrap;
        font-style: italic;
      }
      .front p {
        white-space: nowrap;
        font-size: 8px;
        font-family: monospace;
        color: black;
      }
      .back {
        background-color: orange;
        transform: rotateY(180deg);
      }
    </style>
