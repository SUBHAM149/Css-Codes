        font-family: "Segoe UI", Tahoma, Geneva, Verdana, sans-serif;
        background: #121212;
        display: flex;
        justify-content: center;
        align-items: center;
        height: 100vh;
        margin: 0;
        perspective: 1000px;
      }

      .card-container {
        width: 300px;
        height: 400px;
        perspective: 1500px;
      }

      .card {
        width: 100%;
        height: 100%;
        position: relative;
        transform-style: preserve-3d;
        transition: transform 0.8s cubic-bezier(0.4, 0.2, 0.2, 1);
      }

      .card-container:hover .card {
        transform: rotateY(180deg);
      }

      .card-face {
        position: absolute;
        width: 100%;
        height: 100%;
        backface-visibility: hidden;
        border-radius: 15px;
        overflow: hidden;
        box-shadow: 0 20px 40px rgba(0, 0, 0, 0.4);
      }

      .card-front {
        background: linear-gradient(135deg, #d4b330, #8f94fb);
        color: rgb(21, 19, 19);
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        padding: 20px;
      }

      .card-back {
        background: linear-gradient(135deg, #35cea8, #ff7eb3);
        transform: rotateY(180deg);
        color: rgb(23, 21, 21);
        display: flex;
        align-items: center;
        justify-content: center;
        padding: 20px;
        text-align: center;
      }

      h2 {
        margin: 0;
        font-size: 28px;
      }

      p {
        font-size: 16px;
        margin-top: 10px;
      }
    </style>
