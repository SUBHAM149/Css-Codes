/* text props */
p {
  text-align: justify;
  text-align-last: right;
  text-decoration: underline;
  text-indent: 50px;
  text-transform: uppercase;
}
h1 {
  /* text-decoration: underline overline line-through; */
  text-decoration: underline;
  border-top: 2px solid;
  display: inline;
}
div {
  border: 1px solid;
  width: 200px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

/* font-props
font-size
font-weight
font-family
line-height
letter-spacing
word-spacing
 */
span {
  font-size: 30px;
  font-weight: bolder;
}

#abc {
  color: rgb(0, 89, 255);
  line-height: 22px;
  word-spacing: 24px;
  letter-spacing: 20px;
}
#anchor{
    color: chartreuse;
}
#a1:link{
    color: blueviolet;
}
#a1:visited{
    color: chartreuse;
}
#a1:active{
    color: rgb(255, 0, 111);
}
#b1{
    list-style-position: inside;
    list-style-image: url(../../Assets/Images/html\ logo.webp);
}
body{
    border: 1px solid red;
    height: 100vh;
    background-color: rgb(0, 32, 243);
    background-image: url(../../Assets/Images/krishna\ photo.jpeg);
    background-repeat: no-repeat;
    background-size: 100% 100%;
    background-attachment: fixed;
   
}
